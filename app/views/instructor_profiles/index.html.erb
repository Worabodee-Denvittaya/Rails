<p id="notice"><%= notice %></p>

<h1>Instructor Profiles</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Citizen</th>
      <th>Instructor</th>
      <th>Department name</th>
      <th>User</th>
      <th>Group</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instructor_profiles.each do |instructor_profile| %>
      <tr>
        <td><%= instructor_profile.first_name %></td>
        <td><%= instructor_profile.last_name %></td>
        <td><%= instructor_profile.citizen_id %></td>
        <td><%= instructor_profile.instructor_id %></td>
        <td><%= instructor_profile.department_name %></td>
        <td><%= instructor_profile.user_id %></td>
        <td><%= instructor_profile.group_id %></td>
        <td><%= link_to 'Show', instructor_profile %></td>
        <td><%= link_to 'Edit', edit_instructor_profile_path(instructor_profile) %></td>
        <td><%= link_to 'Destroy', instructor_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Instructor Profile', new_instructor_profile_path %>
